#include<bits/stdc++.h>
 
using namespace std;

int funct1(int a,int b,int c,int d)
{
   int  max1 = (a > b && a > c && a > d) ?
               a : ((b > c && b > d) ?
               b : (c > d ? c : d));
   
     return max1;  
}


int main(){
    
    int a1,b1,c1,d1;
    cin>>a1>>b1>>c1>>d1;
    
    cout<<funct1(a1,b1,c1,d1);
    
    return 0;
}
